{
  "name": "@navikt/ds-tokens",
  "version": "7.0.0",
  "description": "Design-tokens for NAV designsystem",
  "author": "Aksel | NAV designsystem team",
  "keywords": [
    "css",
    "less",
    "javascript",
    "design-tokens"
  ],
  "main": "dist/tokens.css",
  "license": "MIT",
  "files": [
    "/dist",
    "!/dist/darkside",
    "/src",
    "/tailwind",
    "docs.json"
  ],
  "scripts": {
    "build": "node ./config/build.mjs > /dev/null && node ./config/version-tag.mjs",
    "build:darkside": "tsx ./darkside",
    "watch": "nodemon --watch src/index.js --exec \"yarn build\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/core/tokens"
  },
  "devDependencies": {
    "color": "4.2.3",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "nodemon": "^3.0.1",
    "style-dictionary": "^4.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.1.6",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
