{
  "name": "@navikt/ds-icons",
  "version": "0.5.6",
  "private": false,
  "description": "NAVs egne ikoner i SVG og React format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/nav-frontend-moduler/tree/master/%40navikt/ds-icons"
  },
  "keywords": [
    "NAV",
    "designsystem",
    "react"
  ],
  "license": "MIT",
  "author": "NAV",
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "/svg"
  ],
  "scripts": {
    "create-package-json-pointers-to-esm": "node ../../../utilities/scripts/createPackageJsonsWithESMPointers.js",
    "dl-icons": "FILE_KEY=UmEVH3pZ71uJPsSz9ilP3Y node --experimental-modules --es-module-specifier-resolution=node -r dotenv/config figma-api/index.mjs dotenv_config_path=figma-api/.env",
    "create-icons": "svgr --silent --typescript --ref --icon --title-prop --svg-props focusable={false} --replace-attr-values \"#262626=currentColor\" -d src svg",
    "convert-icons": "node figma-api/pngConverter.mjs",
    "clean": "rimraf src png NAV-ikonpakke-png.zip NAV-ikonpakke-svg.zip",
    "zip-icons": "node figma-api/zipIcons.mjs",
    "build": "yarn clean && yarn create-icons && tsc && tsc -p tsconfig.esm.json && yarn create-package-json-pointers-to-esm && yarn run convert-icons && yarn zip-icons"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "4.4.0",
    "react": "^16.8.0 || ^17.0.0",
    "rimraf": "3.0.2",
    "sharp": "0.29.1",
    "zip-dir": "^2.0.0"
  }
}
