{
  "name": "@navikt/ds-css",
  "version": "7.33.2",
  "description": "CSS for Nav Designsystem",
  "author": "Aksel | Nav designsystem team",
  "keywords": [
    "css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "*",
    "!dist/version",
    "!config/bundle.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/core/css"
  },
  "bugs": {
    "url": "https://github.com/navikt/aksel/issues"
  },
  "homepage": "https://aksel.nav.no/designsystemet/",
  "scripts": {
    "build": "node config/bundle.js && yarn build:darkside",
    "css:get-version": "node config/get-version.js",
    "build:darkside": "tsx ./config/bundle.darkside.ts",
    "test": "yarn build && vitest run"
  },
  "devDependencies": {
    "@navikt/ds-tokens": "^7.33.2",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.25.0",
    "cssnano": "7.1.1",
    "esbuild": "^0.25.11",
    "fast-glob": "3.2.11",
    "lightningcss": "^1.30.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.31",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "13.0.2",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "./dist/index.css",
  "exports": {
    ".": "./dist/index.css",
    "./dist/*": "./dist/*",
    "./darkside": "./dist/darkside/index.css",
    "./tokens.json": "./tokens.json",
    "./config/_mappings": "./config/_mappings.js"
  }
}
