{
  "name": "@navikt/ds-tailwind",
  "version": "7.28.0",
  "description": "Tailwind config based on @navikt/ds-tokens",
  "author": "Aksel | Nav designsystem team",
  "keywords": [
    "tailwind",
    "design-tokens"
  ],
  "license": "MIT",
  "files": [
    "tailwind.config.js",
    "tailwind.config.d.ts",
    "tailwind.darkside.config.js"
  ],
  "scripts": {
    "build": "tsx ./src && yarn build:darkside",
    "build:darkside": "tsx darkside.ts",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/core/tailwind"
  },
  "devDependencies": {
    "@navikt/ds-tokens": "^7.28.0",
    "color": "5.0.0",
    "lodash": "^4.17.21",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "5.8.3",
    "vitest": "^2.1.9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "main": "tailwind.config.js",
  "types": "tailwind.config.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./tailwind.config.d.ts",
        "default": "./tailwind.config.js"
      },
      "require": {
        "types": "./tailwind.config.d.ts",
        "default": "./tailwind.config.js"
      }
    },
    "./darkside-tw3": {
      "import": {
        "types": "./tailwind.config.d.ts",
        "default": "./tailwind.darkside.config.js"
      },
      "require": {
        "types": "./tailwind.config.d.ts",
        "default": "./tailwind.darkside.config.js"
      }
    }
  }
}
