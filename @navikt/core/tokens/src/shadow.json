      },
      "small": {
        "value": "0 1px 2px -1px rgba(0, 0, 0, 0.1), 0 1px 3px 0 rgba(0, 0, 0, 0.10)"
      },
      "medium": {
        "value": "0 2px 4px -2px rgba(0, 0, 0, 0.1), 0 4px 6px -1px rgba(0, 0, 0, 0.1)"
      },
      "large": {
        "value": "0 4px 6px -4px rgba(0, 0, 0, 0.1), 0 10px 15px -3px rgba(0, 0, 0, 0.1)"
      },
      "xlarge": {
        "value": "0 8px 10px -6px rgba(0, 0, 0, 0.1), 0 25px 50px -5px rgba(0, 0, 0, 0.1)"
      },
      "focus": {
        "value": "0 0 0 3px {navds.semantic.color.focus.@.value}"
      },
      "focus-inverted": {
        "value": "0 0 0 3px {navds.semantic.color.focus.inverted.value}"
