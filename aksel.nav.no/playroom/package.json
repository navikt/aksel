{
  "name": "playroom",
  "scripts": {
    "start": "yarn sync-imports && playroom start",
    "build:playroom": "yarn clean && yarn sync-imports && playroom build && yarn rewrite-build",
    "sync-imports": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/generate-imports.ts",
    "rewrite-build": "tsx --tsconfig ./scripts/tsconfig.json ./scripts/rewrite-build.ts",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-preval": "^5.1.0",
    "css-loader": "^6.8.1",
    "fast-glob": "3.3.3",
    "playroom": "^1.0.5",
    "rimraf": "6.1.2",
    "style-loader": "^3.3.3",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "react": "19.2.3"
  }
}
