{
  "name": "@navikt/ds-react",
  "version": "1.4.4",
  "description": "NAV designsystem react components",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "_docs.json",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "docgen": "yarn ts-node ../../../scripts/docgen.ts",
    "create-package-json-pointers-to-esm": "node ../../../scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm",
    "build": "concurrently \"tsc\" \"tsc -p tsconfig.esm.json\" && yarn create-package-json-pointers-to-esm",
    "watch": "tsc --watch -p tsconfig.esm.json",
    "test": "TZ=UTC jest"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "0.9.2",
    "@navikt/ds-icons": "^1.4.4",
    "@radix-ui/react-tabs": "1.0.0",
    "@radix-ui/react-toggle-group": "1.0.0",
    "clsx": "^1.2.1",
    "date-fns": "2.29.3",
    "react-day-picker": "8.3.4",
    "react-modal": "3.15.1"
  },
  "devDependencies": {
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/faker": "5.5.8",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.35",
    "@types/react-modal": "^3.13.1",
    "concurrently": "7.2.1",
    "copyfiles": "^2.4.1",
    "faker": "5.5.3",
    "fast-glob": "3.2.11",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "3.0.2",
    "swr": "^1.1.2",
    "ts-jest": "^29.0.0",
    "ts-node": "10.9.0",
    "typescript": "^4.8.0"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
