{
  "name": "aksel-icons",
  "version": "1.0.0",
  "description": "This plugin allows you to search and insert icons from Aksel into your Figma document.",
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=development --watch",
    "prettier:format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}' "
  },
  "dependencies": {
    "@navikt/aksel-icons": "^2.8.2",
    "@navikt/ds-css": "^2.8.5",
    "@navikt/ds-react": "^2.8.5",
    "@navikt/ds-tailwind": "^2.8.5",
    "fuse.js": "^6.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.50.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-dev-utils": "^12.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
