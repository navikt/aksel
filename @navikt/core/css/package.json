{
  "name": "@navikt/ds-css",
  "version": "1.3.28",
  "description": "Css for NAV Designsystem components",
  "author": "NAV Designsystem team",
  "keywords": [
    "css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git"
  },
  "main": "./dist/index.css",
  "scripts": {
    "build": "postcss --use postcss-import postcss-combine-duplicated-selectors -o dist/index.css index.css",
    "watch": "postcss --watch --use postcss-import -o dist/index.css index.css"
  },
  "devDependencies": {
    "@navikt/ds-tokens": "^1.3.28",
    "normalize.css": "^8.0.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-import": "^14.0.2"
  }
}
