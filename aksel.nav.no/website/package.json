{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Aksel.nav.no website",
  "author": "Aksel | NAV designsystem team",
  "scripts": {
    "dev": "next | pino-pretty",
    "build:old": "next build",
    "build": "yarn workspace playroom build && next build && yarn update:searchindex && yarn cp:assets",
    "start": "cd .next/standalone/aksel.nav.no/website && PORT=3000 HOSTNAME=0.0.0.0 node server.js",
    "cp:playroom": "cross-env mkdir public/playroom && cross-env cp -R ../playroom/dist/* public/playroom",
    "cp:static": "cross-env cp -R .next/static .next/standalone/aksel.nav.no/website/.next/static",
    "cp:public": "cross-env cp -R public .next/standalone/aksel.nav.no/website/",
    "cp:assets": "yarn cp:playroom && yarn cp:public && yarn cp:static",
    "update:searchindex": "ts-node --project tsconfig.json ./scripts/create-index",
    "e2e": "yarn build && yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:full": "yarn build && ts-node ./e2e/genUrls.ts && FULL_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:ci": " yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:ci-full": "ts-node ./e2e/genUrls.ts && FULL_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "update:props": "ts-node --project tsconfig.tsnode.json ./scripts/update-props",
    "update:tokens": "ts-node --project tsconfig.tsnode.json ./scripts/update-tokens",
    "update:eksempler": "ts-node --project tsconfig.tsnode.json ./scripts/examples/update-sanity",
    "update:changelog": "ts-node --project tsconfig.tsnode.json ./scripts/changelog",
    "update:sanity": "yarn update:tokens && yarn update:props && yarn update:eksempler && yarn update:changelog",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "jest",
    "playwright": "playwright test",
    "test:watch": "jest --watch",
    "backup": "ts-node scripts/backup.js",
    "watch:tw": "yarn tailwindcss -i ./styles/index.css -o ./dist/tw.css --watch"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.2.3",
    "@portabletext/react": "^3.0.7",
    "@sanity/client": "^6.6.0",
    "@sanity/code-input": "4.1.1",
    "@sanity/color-input": "^3.1.0",
    "@sanity/image-url": "1.0.2",
    "@sanity/table": "1.0.1",
    "@sanity/vision": "3.18.0",
    "boring-avatars": "1.10.1",
    "clsx": "^1.2.1",
    "codesandbox": "^2.2.3",
    "date-fns": "2.29.3",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "next": "^13.2.1",
    "next-logger": "3.0.2",
    "next-sanity": "5.5.8",
    "pino": "^8.14.1",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hotjar": "^6.1.0",
    "react-responsive-masonry": "^2.1.7",
    "sanity": "3.18.0",
    "sanity-plugin-asset-source-unsplash": "^1.1.1",
    "sanity-plugin-media": "^2.2.2",
    "sharp": "0.32.5",
    "styled-components": "^6.0.0",
    "swr": "^1.1.2",
    "tailwindcss": "^3.3.3",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.5.2",
    "@babel/core": "^7.19.0",
    "@next/bundle-analyzer": "^13.2.1",
    "@next/eslint-plugin-next": "^13.2.1",
    "@playwright/test": "1.35.1",
    "@sanity/block-tools": "^3.18.0",
    "@sanity/schema": "^3.18.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.0",
    "babel-plugin-preval": "^5.1.0",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jsdom": "^22.1.0",
    "json5": "2.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.31",
    "postcss-import": "^14.1.0",
    "postcss-loader": "7.0.0",
    "showdown": "^2.1.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
