{
  "name": "@navikt/aksel",
  "version": "2.8.15",
  "description": "Aksel client-tool for css-imports, codemods (planned) and more",
  "author": "Aksel | NAV designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "cli",
    "css",
    "CDN"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch -p tsconfig.json",
    "test": "yarn build && jest",
    "test:watch": "jest --watch"
  },
  "bin": {
    "aksel": "./dist/help.js",
    "css-imports": "./dist/css-imports/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/aksel"
  },
  "type": "module",
  "homepage": "https://github.com/navikt/aksel/tree/main/@navikt/aksel",
  "dependencies": {
    "@navikt/ds-css": "2.8.15",
    "axios": "1.3.6",
    "chalk": "4.1.0",
    "clipboardy": "^3.0.0",
    "enquirer": "^2.3.6",
    "fast-glob": "3.2.11",
    "lodash": "4.17.21",
    "react-scanner": "^1.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "typescript": "^4.8.0",
    "jest": "^29.0.0"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/unified/*"
    ],
    "testMatch": [
      "**/__tests__/*.js"
    ]
  }
}
