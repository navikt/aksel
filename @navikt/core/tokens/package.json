{
  "name": "@navikt/ds-tokens",
  "version": "8.0.1",
  "description": "Design-tokens for Nav designsystem",
  "author": "Aksel | Nav designsystem team",
  "keywords": [
    "css",
    "less",
    "javascript",
    "design-tokens"
  ],
  "license": "MIT",
  "files": [
    "/dist",
    "types.ts",
    "internal-types.ts",
    "token_docs.js"
  ],
  "scripts": {
    "build": "tsx ./src && yarn build:figma-config && yarn build:plugin && tsx ./config/version-tag.ts",
    "build:figma-config": "tsx ./src/figma",
    "build:plugin-dev": "esbuild src/figma/plugin/plugin.ts --target=es2016 --bundle --define:process.env.NODE_ENV=\\\"development\\\" --outfile='src/figma/plugin/plugin.js'",
    "build:plugin": "esbuild src/figma/plugin/plugin.ts --target=es2016 --bundle --define:process.env.NODE_ENV=\\\"production\\\" --outfile='src/figma/plugin/plugin.js'",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/core/tokens"
  },
  "bugs": {
    "url": "https://github.com/navikt/aksel/issues"
  },
  "homepage": "https://aksel.nav.no/grunnleggende/styling/design-tokens/",
  "devDependencies": {
    "@figma/plugin-typings": "^1.100.2",
    "colorjs.io": "^0.6.0",
    "esbuild": "^0.25.11",
    "lightningcss": "^1.30.1",
    "lodash": "4.17.21",
    "style-dictionary": "^4.1.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "main": "dist/tokens.css",
  "exports": {
    ".": "./dist/tokens.css",
    "./types": "./types.ts",
    "./token_docs": "./token_docs.js",
    "./dist/*": {
      "types": "./dist/tokens.d.ts",
      "default": "./dist/*"
    },
    "./css": "./dist/tokens.css",
    "./js": {
      "types": "./dist/tokens.d.ts",
      "import": "./dist/tokens.js",
      "require": "./dist/tokens-cjs.js"
    },
    "./scss": "./dist/tokens.scss",
    "./less": "./dist/tokens.less"
  }
}
