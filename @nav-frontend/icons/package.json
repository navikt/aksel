{
  "name": "@nav-frontend/icons",
  "version": "0.1.0-beta.1",
  "license": "MIT",
  "author": "NAV",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/src",
    "/svg"
  ],
  "scripts": {
    "create-icons": "svgr --template template/reactTemplate.js -d src svg --icon --typescript --ref --replace-attr-values '#3E3832=currentColor'",
    "build": "rm -rf src && rm -rf lib && npm run create-icons && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/nav-frontend-icons.git"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "@svgr/cli": "^5.4.0"
  }
}
