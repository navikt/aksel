{
  "name": "@navikt/ds-react",
  "version": "0.6.9",
  "private": false,
  "description": "NAV designsystem react components",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/nav-frontend-moduler/tree/master/%40navikt/ds-react"
  },
  "main": "esm/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/cjs",
    "/esm",
    "/src"
  ],
  "scripts": {
    "cjs-package": "copyfiles -f config/package.json cjs",
    "clean": "rm -rf cjs esm",
    "build": "yarn run clean && tsc && tsc -p tsconfig-esm.json && yarn cjs-package",
    "watch": "tsc --watch -p tsconfig-esm.json"
  },
  "dependencies": {
    "@navikt/ds-icons": "^0.3.3",
    "@popperjs/core": "^2.5.4",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "react-collapse": "^5.1.0",
    "react-merge-refs": "^1.1.0",
    "react-modal": "3.12.1",
    "react-popper": "^2.2.4",
    "uuid": "^8.3.2",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "react-router-dom": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  }
}
