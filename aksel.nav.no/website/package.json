{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Aksel.nav.no website",
  "author": "Aksel | NAV designsystem team",
  "scripts": {
    "dev": "next",
    "build:old": "next build",
    "build": "next build && yarn cp:assets",
    "start": "cd .next/standalone/aksel.nav.no/website && node server.js",
    "cp:assets": "cross-env cp -R public .next/standalone/aksel.nav.no/website/ && cross-env cp -R .next/static .next/standalone/aksel.nav.no/website/.next/static",
    "e2e": "yarn build && NEXT_PUBLIC_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:full": "yarn build && ts-node ./e2e/genUrls.ts && NEXT_PUBLIC_TEST=true FULL_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:ci": "NEXT_PUBLIC_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "e2e:ci-full": "ts-node ./e2e/genUrls.ts && NEXT_PUBLIC_TEST=true FULL_TEST=true yarn start-server-and-test start http://localhost:3000 playwright",
    "update:props": "ts-node --project tsconfig.tsnode.json ./scripts/update-props",
    "update:tokens": "ts-node --project tsconfig.tsnode.json ./scripts/update-tokens",
    "update:eksempler": "ts-node --project tsconfig.tsnode.json ./scripts/examples/update-sanity",
    "update:sanity": "yarn update:tokens && yarn update:props && yarn update:eksempler",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "jest",
    "playwright": "playwright test",
    "test:watch": "jest --watch",
    "backup": "ts-node scripts/backup.js",
    "watch:tw": "yarn tailwindcss -i ./styles/index.css -o ./dist/tw.css --watch"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^6.1.2",
    "@sanity/code-input": "4.1.1",
    "@sanity/color-input": "^3.1.0",
    "@sanity/icons": "2.3.1",
    "@sanity/image-url": "1.0.2",
    "@sanity/preview-kit": "^1.5.4",
    "@sanity/table": "1.0.1",
    "@sanity/ui": "^1.3.3",
    "@sanity/vision": "3.11.5",
    "@svgr/webpack": "^5.5.0",
    "amplitude-js": "^8.21.5",
    "boring-avatars": "1.7.0",
    "clsx": "^1.2.1",
    "codesandbox": "^2.2.3",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "2.29.3",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "next": "^13.2.1",
    "next-sanity": "^4.3.3",
    "prism-react-renderer": "^1.3.0",
    "prismjs": "^1.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-modal": "3.15.1",
    "react-responsive-masonry": "^2.1.7",
    "recharts": "^2.2.0",
    "sanity": "3.11.5",
    "sanity-plugin-asset-source-unsplash": "^1.1.0",
    "sanity-plugin-media": "^2.0.5",
    "sharp": "0.31.3",
    "sitemap": "^7.0.0",
    "styled-components": "^5.2",
    "swr": "^1.1.2",
    "tailwindcss": "^3.2.0",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.5.2",
    "@babel/core": "^7.19.0",
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.2.0",
    "@playwright/test": "1.33.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.0",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "json5": "2.2.1",
    "postcss": "^8.4.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "7.0.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.0",
    "ts-node": "10.9.0",
    "typescript": "^4.8.0"
  }
}
