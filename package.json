{
  "name": "nav-ds",
  "description": "Repository for NAV Designsystem",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn workspace website start",
    "clean": "yarn workspace website clean",
    "build:gatsby": "yarn workspace website build:gatsby",
    "serve": "yarn workspace website serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lerna:watch": "yarn run lerna run watch --parallel",
    "lerna:build": "yarn run lerna run build",
    "lint": "yarn lint:jsts && yarn lint:less",
    "lint:jsts": "npx eslint .",
    "lint:less": "npx stylelint --syntax less **/*.less",
    "gulp:build": "yarn run gulp --gulpfile utilities/build/gulpfile.js build --ignoreErrors",
    "clean:lib": "node utilities/scripts/clean.js",
    "boot": "yarn install && yarn clean:lib && yarn gulp:build && yarn install --frozen-lockfile",
    "fix:versions": "node utilities/scripts/fixDependencyVersions.js",
    "chromatic": "npx chromatic --project-token x3xqdfgkujg"
  },
  "workspaces": [
    "website",
    "packages/*",
    "./utilities/build/props-resolver"
  ],
  "prettier": {},
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "ignorePatterns": [
      "node_modules",
      "lib"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "value-list-comma-newline-after": null,
      "declaration-colon-newline-after": null,
      "no-descending-specificity": null
    },
    "ignoreFiles": [
      "packages/nav-frontend-core/less/vendor/_normalize.less",
      "packages/nav-frontend-core/less/_variabler-exports.less",
      "packages/nav-frontend-core/less/hide-text.less"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/nav-frontend-moduler.git"
  },
  "dependencies": {
    "css-loader": "^4.3.0",
    "cypress": "^5.3.0",
    "depth-first": "^4.0.0",
    "gatsby-transformer-react-docgen-typescript-custom": "0.1.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-util": "^3.0.8",
    "lerna": "^3.22.1",
    "less": "3.12.2",
    "less-loader": "^7.0.1",
    "less-vars-to-js": "^1.3.0",
    "lodash.camelcase": "^4.3.0",
    "react-docgen": "^5.3.0",
    "react-docgen-exports-handler": "^1.0.0",
    "start-server-and-test": "^1.11.4",
    "style-loader": "^1.2.1",
    "through2": "^2.0.3",
    "vinyl": "^2.2.1",
    "gulp-newer": "^1.3.0",
    "gulp-babel": "^6.1.2",
    "gulp-typescript": "^3.2.3",
    "gulp-plumber": "^1.1.0",
    "gulp-if": "^3.0.0",
    "chalk": "^1.1.3",
    "gulp-cssfont64-formatter": "0.0.2",
    "merge2": "^1.2.0",
    "react-svg-icon-generator-fork": "^1.0.2",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-react-docgen": "^1.8.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.2.0",
    "eslint": "^7.10.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-transformer-react-docgen-typescript-custom": "0.1.0",
    "gulp-insert": "^0.5.0",
    "lerna-changelog": "^1.0.1",
    "prettier": "2.1.2",
    "react-is": "^16.13.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^3.7.3"
  }
}
