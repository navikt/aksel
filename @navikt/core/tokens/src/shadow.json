{
  "navds": {
    "global": {
      "shadow": {
        "xs": {
          "value": "0px 2px 1px -1px rgb(0, 0, 0, 0.1), 0px 1px 3px 0px rgb(0, 0, 0, 0.1)"
        },
        "sm": {
          "value": "0px 2px 4px -2px rgb(0, 0, 0, 0.1), 0px 4px 6px -1px rgb(0, 0, 0, 0.1)"
        },
        "md": {
          "value": "0px 4px 6px -4px rgb(0, 0, 0, 0.1) ,0px 10px 15px -3px rgb(0, 0, 0, 0.1)"
        },
        "lg": {
          "value": "0px 8px 10px -6px rgb(0, 0, 0, 0.1), 0px 20px 25px -5px rgb(0, 0, 0, 0.1)"
        },
        "xl": {
          "value": "0px 8px 10px -6px rgb(0, 0, 0, 0.1), 0px 25px 50px -12px rgb(0, 0, 0, 0.25)"
        },
        "inner": {
          "value": "0px 2px 4px 0px rgb(0, 0, 0, 0.05) inset, 0px 1px 0px 0px rgb(0, 0, 0, 0.1) inset"
        },
        "focus": {
          "value": "0 0 0 3px {navds.semantic.color.focus.@.value}"
        },
        "focus-inverted": {
          "value": "0 0 0 3px {navds.semantic.color.focus.inverted.value}"
        },
        "popover": {
          "value": "0 2px 4px 0 {navds.global.color.gray.200.value}"
        },
        "card": {
          "value": "0 1px 3px 0 rgba(38,38,38,0.2),0 2px 1px 0 rgba(38,38,38,0.12),0 1px 1px 0 rgba(38,38,38,0.14)"
        }
      }
    }
  }
}
