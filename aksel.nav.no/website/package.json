{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Aksel.nav.no website",
  "author": "Aksel | Nav designsystem team",
  "scripts": {
    "dev": "next --turbopack | pino-pretty",
    "build:old": "next build",
    "build": "yarn workspace playroom build:playroom && yarn update:searchindex && next build && yarn update:rss && yarn cp:assets",
    "start": "cd .next/standalone/aksel.nav.no/website && PORT=3000 HOSTNAME=0.0.0.0 node server.js",
    "update:searchindex": "yarn tsx --env-file-if-exists=.env ./scripts/create-index",
    "e2e": "yarn build && yarn start-server-and-test 'yarn start' http://localhost:3000 'yarn playwright'",
    "e2e:full": "yarn build && yarn generate-testpaths && start-server-and-test start http://localhost:3000 playwright",
    "e2e:ci-full": "yarn generate-testpaths && start-server-and-test start http://localhost:3000 playwright",
    "update:rss": "yarn tsx --env-file-if-exists=.env ./scripts/generate-rss-feed",
    "update:props": "yarn tsx --env-file-if-exists=.env ./scripts/update-props",
    "update:tokens": "yarn tsx --env-file-if-exists=.env ./scripts/update-tokens",
    "update:eksempler": "yarn tsx --env-file-if-exists=.env ./scripts/update-examples/update-sanity",
    "validate:eksempler": "yarn tsx ./scripts/update-examples/validate",
    "update:changelog": "yarn tsx --env-file-if-exists=.env ./scripts/changelog",
    "update:sanity": "yarn update:tokens && yarn update:props && yarn update:eksempler && yarn update:changelog",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "vitest run -c config/vitest.config.ts",
    "test:watch": "vitest watch -c config/vitest.config.ts",
    "playwright": "playwright test",
    "backup": "yarn tsx --env-file-if-exists=.env scripts/backup.ts",
    "generate-testpaths": "yarn tsx --env-file-if-exists=.env ./scripts/generate-testpaths.ts",
    "watch:tw": "yarn tailwindcss -i ./components/styles/index.css -o ./dist/tw.css --watch",
    "update:sanity-schema": "cd ./sanity && yarn sanity schema extract --workspace default && yarn rename:sanity-schema && yarn sanity typegen generate",
    "###⬆️ public (devs probably want these)": "###⬇️ private (ignore these)",
    "rename:sanity-schema": "cd ./sanity/ && node -e \"require('fs').rename('./schema.json', './sanity-schema.json', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "cp:playroom": "copyfiles -E -u 3 '../playroom/dist/sandbox/**' public/",
    "cp:static": "copyfiles -E -u 1 '.next/static/**' .next/standalone/aksel.nav.no/website/.next/",
    "cp:public": "copyfiles -E 'public/**' .next/standalone/aksel.nav.no/website/",
    "cp:assets": "yarn cp:playroom && yarn cp:public && yarn cp:static"
  },
  "dependencies": {
    "@navikt/next-logger": "2.0.0",
    "@navikt/oasis": "3.2.1",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^6.28.2",
    "@sanity/code-input": "4.1.4",
    "@sanity/color-input": "^4.0.3",
    "@sanity/image-url": "1.1.0",
    "@sanity/locale-nb-no": "^1.1.19",
    "@sanity/preview-url-secret": "^2.1.5",
    "@sanity/table": "1.1.3",
    "@sanity/vision": "3.78.0",
    "@slack/web-api": "^7.8.0",
    "boring-avatars": "1.10.1",
    "clsx": "^2.1.0",
    "codesandbox-import-utils": "^2.2.3",
    "date-fns": "^4.0.0",
    "fuse.js": "^7.0.0",
    "jscodeshift": "^0.15.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "next": "15.2.3",
    "next-logger": "^5.0.1",
    "next-sanity": "9.8.61",
    "node-cache": "^5.1.2",
    "pino": "^8.21.0",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.53.0",
    "react-markdown": "^9.0.1",
    "react-responsive-masonry": "2.2.0",
    "react-rx": "^2.1.3",
    "remark-gfm": "^4.0.0",
    "sanity": "3.78.0",
    "sanity-plugin-iframe-pane": "^3.2.1",
    "sanity-plugin-media": "^2.3.2",
    "server-only": "^0.0.1",
    "sharp": "0.33.5",
    "styled-components": "^6.0.0",
    "swr": "^2.3.2",
    "tailwindcss": "^3.3.3",
    "tinycolor2": "^1.6.0",
    "typescript-cookie": "^1.0.6",
    "zod": "^3.22.4"
  },
  "_note": "When upgrading @axe-core/playwright, try removing the resolution entry in the root package.json",
  "devDependencies": {
    "@axe-core/playwright": "4.10.0",
    "@babel/core": "^7.19.0",
    "@next/bundle-analyzer": "15.2.3",
    "@next/eslint-plugin-next": "15.2.3",
    "@playwright/test": "1.48.1",
    "@sanity/block-tools": "3.70.0",
    "@sanity/schema": "3.78.0",
    "@types/jscodeshift": "^0.11.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.0.0",
    "@types/umami": "^2.10.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.0",
    "jsdom": "25.0.1",
    "json5": "2.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.31",
    "postcss-import": "^14.1.0",
    "postcss-loader": "7.0.0",
    "rss": "^1.2.2",
    "showdown": "^2.1.0",
    "start-server-and-test": "^2.0.3",
    "tsx": "^4.19.1",
    "typescript": "5.5.4",
    "vitest": "^2.1.9"
  }
}
