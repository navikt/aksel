{
  "name": "@navikt/ds-css",
  "version": "8.2.1",
  "description": "CSS for Nav Designsystem",
  "author": "Aksel | Nav designsystem team",
  "keywords": [
    "css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "*",
    "!dist/version",
    "!config/bundle.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/core/css"
  },
  "bugs": {
    "url": "https://github.com/navikt/aksel/issues"
  },
  "homepage": "https://aksel.nav.no/designsystemet/",
  "scripts": {
    "build": "tsx ./config/bundle.ts",
    "css:get-version": "node config/get-version.js"
  },
  "devDependencies": {
    "@navikt/ds-tokens": "^8.2.1",
    "browserslist": "^4.25.0",
    "esbuild": "^0.25.11",
    "fast-glob": "3.3.3",
    "lightningcss": "^1.30.1",
    "normalize.css": "^8.0.1",
    "tsx": "^4.20.6"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "./dist/index.css",
  "exports": {
    ".": "./dist/index.css",
    "./dist/*": "./dist/*",
    "./tokens.json": "./tokens.json",
    "./config/_mappings": "./config/_mappings.js"
  }
}
