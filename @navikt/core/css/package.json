{
  "name": "@navikt/ds-css",
  "version": "7.8.0",
  "description": "CSS for NAV Designsystem",
  "author": "Aksel | NAV designsystem team",
  "keywords": [
    "css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "*",
    "!dist/version",
    "!config/bundle.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git"
  },
  "scripts": {
    "build": "node config/bundle.js && yarn build:darkside",
    "watch": "nodemon --ignore dist -e css --watch . --exec \"yarn build\"",
    "css:get-version": "node config/get-version.js",
    "build:darkside": "tsx ./config/bundle.darkside.ts",
    "test": "yarn build && vitest run"
  },
  "devDependencies": {
    "@navikt/ds-tokens": "^7.8.0",
    "@types/clean-css": "4.2.11",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.2",
    "clean-css": "5.3.3",
    "cssnano": "6.0.0",
    "fast-glob": "3.2.11",
    "lightningcss": "^1.28.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^9.0.0",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-import": "^14.1.0",
    "tsx": "^4.7.1",
    "vitest": "^1.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "./dist/index.css",
  "exports": {
    ".": "./dist/index.css",
    "./dist/*": "./dist/**/*.css",
    "./darkside": "./dist/darkside/index.min.css"
  }
}
