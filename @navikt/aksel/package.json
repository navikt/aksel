{
  "name": "@navikt/aksel",
  "version": "5.15.0",
  "description": "Aksel command line interface. Handles css-imports, codemods and more",
  "author": "Aksel | NAV designsystem team",
  "license": "MIT",
  "keywords": [
    "Commandline",
    "CLI",
    "CDN",
    "Codemods"
  ],
  "files": [
    "dist",
    "!dist/**/tests"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc -p tsconfig.json",
    "dev": "tsc --watch -p tsconfig.json",
    "test": "yarn build && NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules yarn jest --watch"
  },
  "bin": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/aksel"
  },
  "homepage": "https://aksel.nav.no/grunnleggende/kode/kommandolinje",
  "dependencies": {
    "@navikt/ds-css": "5.15.0",
    "axios": "1.6.0",
    "chalk": "4.1.0",
    "clipboardy": "^2.3.0",
    "commander": "10.0.1",
    "enquirer": "^2.3.6",
    "fast-glob": "3.2.11",
    "figlet": "1.6.0",
    "is-git-clean": "1.1.0",
    "jscodeshift": "^0.15.1",
    "jscodeshift-add-imports": "1.0.10",
    "lodash": "4.17.21",
    "react-scanner": "^1.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "^5.1.6"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
