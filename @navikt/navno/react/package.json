{
  "name": "@navikt/ds-react-navno",
  "version": "0.1.0",
  "private": false,
  "description": "React components for External pages in NAV",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/nav-frontend-moduler"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/cjs",
    "/esm",
    "/src"
  ],
  "scripts": {
    "create-package-json-pointers-to-esm": "node ../../../utilities/scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm src/card",
    "build": "yarn run clean && yarn build:cards && tsc && tsc -p tsconfig.esm.json && yarn create-package-json-pointers-to-esm",
    "build:cards": "ts-node buildCards.ts",
    "watch": "tsc --watch -p tsconfig.esm.json"
  },
  "dependencies": {
    "@navikt/ds-icons": "^0.7.0",
    "@navikt/ds-react": "^0.14.1",
    "classnames": "^2.3.1",
    "lottie-web": "^5.8.1"
  },
  "devDependencies": {
    "@types/mustache": "^4.1.2",
    "rimraf": "^3.0.2",
    "mustache": "^4.2.0",
    "ts-node": "^10.4.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  }
}
