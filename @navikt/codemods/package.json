{
  "name": "@navikt/ds-codemod",
  "version": "1.0.0",
  "bin": "./codemod.js",
  "private": false,
  "description": "NAV designsystem codemods",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "codemod",
    "jscodeshift"
  ],
  "scripts": {
    "test": "cd ../../ && cross-env NODE_ENV=test mocha 'packages/mui-codemod/**/*.test.js'",
    "prebuild": "rimraf build",
    "build": "node ../../scripts/build node --out-dir ./build && cpy README.md build && cpy package.json build && cpy codemod.js build",
    "release": "yarn build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git",
    "directory": "@navikt/codemods"
  },
  "homepage": "https://github.com/navikt/Designsystemet/tree/master/@navikt/codemods",
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@babel/runtime": "^7.17.2",
    "@babel/traverse": "^7.17.3",
    "jscodeshift": "^0.13.1",
    "jscodeshift-add-imports": "^1.0.10",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jscodeshift": "0.11.5"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  }
}
