    "copyfiles": "^2.4.1",
    "faker": "^5.5.3",
    "jest": "^27.2.0",
    "react-router-dom": "^6.1.0",
    "rimraf": "3.0.2",
    "styled-components": "^5.3.1",
    "ts-jest": "^27.0.5"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30",
    "react": "^17.0.0"
  }
