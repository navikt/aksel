{
  "name": "@navikt/ds-tokens",
  "version": "1.5.6",
  "description": "Auto-generated design-tokens for NAV design-systems framework",
  "author": "NAV Designsystem team",
  "keywords": [
    "css",
    "less",
    "javascript",
    "design-tokens"
  ],
  "main": "dist/tokens.css",
  "license": "MIT",
  "files": [
    "/dist",
    "/src",
    "/tailwind",
    "docs.json"
  ],
  "scripts": {
    "build": "node ./config/build.js && yarn validate-docs",
    "watch": "nodemon --watch src/index.js --exec \"yarn build\"",
    "token": "cd config && style-dictionary build",
    "test": "jest",
    "validate-docs": "node config/validate-docs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git",
    "directory": "@navikt/core/tokens"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.35",
    "color": "4.2.3",
    "jest": "^29.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.12",
    "style-dictionary": "^3.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "10.9.0",
    "typescript": "^4.8.0"
  }
}
