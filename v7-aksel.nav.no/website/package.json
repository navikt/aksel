{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Aksel.nav.no website",
  "author": "Aksel | Nav designsystem team",
  "scripts": {
    "dev": "next | pino-pretty --messageKey=message",
    "dev-turbo": "next --turbo | pino-pretty --messageKey=message",
    "build": "concurrently \"yarn workspace playroom build:playroom\" \"yarn next build\" && yarn cp:assets",
    "start": "cd .next/standalone/v7-aksel.nav.no/website && PORT=3000 HOSTNAME=0.0.0.0 node server.js",
    "test": "vitest run -c config/vitest.config.ts",
    "test:watch": "vitest watch -c config/vitest.config.ts",
    "cp:playroom": "copyfiles -E -u 3 '../playroom/dist/sandbox/**' public/",
    "cp:static": "copyfiles -E -u 1 '.next/static/**' .next/standalone/v7-aksel.nav.no/website/.next/",
    "cp:public": "copyfiles -E 'public/**' .next/standalone/v7-aksel.nav.no/website/",
    "cp:assets": "yarn cp:playroom && yarn cp:public && yarn cp:static",
    "prepare:local": "yarn tsx --env-file-if-exists=.env ./scripts/cloneDataset.ts && yarn tsx --env-file-if-exists=.env ./scripts/downloadAssets.ts"
  },
  "dependencies": {
    "@navikt/next-logger": "4.4.1",
    "@sanity/client": "^7.12.0",
    "clsx": "^2.1.0",
    "codesandbox-import-utils": "^2.2.3",
    "date-fns": "^4.0.0",
    "fuse.js": "^7.0.0",
    "groq-js": "^1.21.0",
    "isomorphic-dompurify": "2.26.0",
    "lodash": "^4.17.21",
    "next": "15.5.7",
    "next-logger": "^5.0.1",
    "next-sanity": "11.5.0",
    "next-themes": "^0.4.6",
    "node-cache": "^5.1.2",
    "pino": "^8.21.0",
    "prism-react-renderer": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.1",
    "react-responsive-masonry": "2.2.0",
    "remark-gfm": "^4.0.0",
    "sanity": "4.14.1",
    "server-only": "^0.0.1",
    "sharp": "0.33.5",
    "tailwindcss": "^3.3.3",
    "tinycolor2": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@next/bundle-analyzer": "15.4.5",
    "@next/eslint-plugin-next": "15.4.5",
    "@types/lodash": "4.17.20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "9.2.1",
    "copyfiles": "^2.4.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  }
}
