{
  "name": "@navikt/ds-navno",
  "version": "0.0.1",
  "private": true,
  "description": "nav.no + ds components",
  "author": "Aksel | NAV designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/css",
    "/cjs",
    "/esm",
    "/src",
    "_docs.json",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "docgen": "yarn ts-node ../../../scripts/docgen.ts",
    "create-package-json-pointers-to-esm": "node ../../../scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm css",
    "build": "yarn build-pict && concurrently \"tsc -b\" \"rollup -c\" && yarn css && yarn create-package-json-pointers-to-esm",
    "css": "copyfiles -f esm/index.css css/ && rimraf esm/*.css cjs/*.css",
    "test-ignore": "TZ=UTC jest",
    "build-pict": "svgr --silent --index-template index-template.js --out-dir src/pictograms pictograms"
  },
  "dependencies": {
    "@navikt/ds-icons": "^2.0.5",
    "@navikt/ds-react": "^2.0.5",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@svgr/cli": "6.2.1",
    "@types/node": "^17.0.35",
    "concurrently": "7.2.1",
    "copyfiles": "2.4.1",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-import": "14.1.0",
    "rimraf": "^3.0.2",
    "rollup": "3.2.3",
    "rollup-plugin-node-externals": "5.0.1",
    "rollup-plugin-postcss": "4.0.2",
    "ts-node": "10.9.0",
    "typescript": "^4.8.0"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
