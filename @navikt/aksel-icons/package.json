{
  "name": "@navikt/aksel-icons",
  "version": "2.1.7",
  "author": "Aksel | NAV",
  "homepage": "https://aksel.nav.no/ikoner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git",
    "directory": "@navikt/aksel-icons"
  },
  "keywords": [
    "Icons",
    "NAV",
    "Aksel",
    "designsystem",
    "react",
    "svg"
  ],
  "sideEffects": [
    "*.svg"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "/svg",
    "/meta.json",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "create-icons": "svgr --silent --out-dir src icons",
    "copy-util": "copyfiles util/* src",
    "build": "yarn copy-util && yarn create-icons && concurrently \"tsc\" \"tsc -p tsconfig.esm.json\" && node cleanTypes.js",
    "build:old": "copyfiles util/* src && yarn create-icons && concurrently \"tsc\" \"tsc -p tsconfig.esm.json\" && node cleanTypes.js && yarn create-package-json-pointers-to-esm",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@svgr/cli": "6.2.1",
    "concurrently": "7.2.1",
    "copyfiles": "^2.4.1",
    "fast-glob": "3.2.11",
    "hast-util-select": "5.0.5",
    "jest": "^29.0.0",
    "js-yaml": "^4.1.0",
    "react": "^18.0.0",
    "rehype-parse": "8.0.4",
    "typescript": "^4.8.0",
    "unified": "10.1.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/unified/*"
    ],
    "testMatch": [
      "**/__tests__/*.js",
      "**/__tests__/*.mjs"
    ]
  }
}
