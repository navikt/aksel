{
  "name": "@navikt/ds-react",
  "version": "5.18.3",
  "description": "Aksel react-components for NAV designsystem",
  "author": "Aksel | NAV designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aksel.git"
  },
  "sideEffects": false,
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "!**/*.tsbuildinfo"
  ],
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "docgen": "yarn ts-node ../../../scripts/docgen.ts",
    "write-packagejson": "echo '{\"type\": \"module\"}' > esm/package.json",
    "clean": "rimraf cjs esm",
    "build": "concurrently \"tsc -p tsconfig.build.json\" \"tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json && yarn write-packagejson\" ",
    "watch": "tsc --watch -p tsconfig.esm.json",
    "test": "TZ=UTC jest"
  },
  "dependencies": {
    "@floating-ui/react": "0.25.4",
    "@navikt/aksel-icons": "^5.18.3",
    "@navikt/ds-tokens": "^5.18.3",
    "@radix-ui/react-tabs": "1.0.0",
    "@radix-ui/react-toggle-group": "1.0.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "react-day-picker": "8.10.0"
  },
  "devDependencies": {
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/faker": "5.5.8",
    "@types/jest": "^29.0.0",
    "concurrently": "7.2.1",
    "copyfiles": "^2.4.1",
    "faker": "5.5.3",
    "fast-glob": "3.2.11",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "3.0.2",
    "swr": "^1.1.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "1.8.8",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
