    "./index.js"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc -p .",
    "watch:lint": "tsc --watch -p ."
  },
  "devDependencies": {
    "postcss-media-query-parser": "^0.2.3",
    "postcss-value-parser": "^4.2.0",
    "stylelint": "^14.8.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/unified/*"
    ],
    "testMatch": [
      "**/__tests__/*.js",
      "**/__tests__/*.mjs"
    ]
