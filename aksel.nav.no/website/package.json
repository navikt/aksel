{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Nextjs page for sanity docs",
  "scripts": {
    "postinstall": "playwright install --with-deps",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "e2e": "yarn generate:testurls && yarn build && yarn start-server-and-test start http://localhost:3000 playwright",
    "update:colors": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-color-examples",
    "update:props": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-props",
    "update:tokens": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/tokens",
    "update:search": "npx ts-node --project tsconfig.tsnode.json ./lib/search",
    "update:eksempler": "npx ts-node --project tsconfig.tsnode.json ./lib/examples/update-sanity",
    "update:sanity": "yarn update:tokens && yarn update:props && yarn update:colors && yarn update:eksempler",
    "generate:testurls": "npx ts-node --project tsconfig.tsnode.json ./lib/testing/generateTestUrls",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "playwright": "playwright test",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@docsearch/css": "3.3.0",
    "@docsearch/react": "3.3.0",
    "@radix-ui/react-tabs": "1.0.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^3.3.0",
    "@sanity/image-url": "1.0.1",
    "@svgr/webpack": "^5.5.0",
    "algoliasearch": "4.12.2",
    "amplitude-js": "^8.8.0",
    "classnames": "^2.3.1",
    "codesandbox": "^2.2.3",
    "color": "4.2.3",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "2.29.3",
    "elevator.js": "1.0.1",
    "jose": "4.10.0",
    "js-file-download": "^0.4.12",
    "jwt-decode": "3.1.2",
    "lodash": "^4.17.21",
    "next": "^12.2.0",
    "next-sanity": "^1.0.0",
    "prettier": "2.7.1",
    "prism-react-renderer": "^1.3.0",
    "prismjs": "^1.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-instantsearch-hooks-web": "^6.26.0",
    "react-modal": "3.15.1",
    "sitemap": "^7.0.0",
    "tailwindcss": "^3.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.2.0",
    "@playwright/test": "^1.27.0",
    "@sanity-codegen/types": "1.0.0-alpha.13",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.0",
    "cross-env": "^7.0.0",
    "css": "^3.0.0",
    "dotenv": "^10.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "json5": "2.2.1",
    "nanoid": "3.0.0",
    "next-transpile-modules": "^9.0.0",
    "postcss": "^8.4.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "7.0.0",
    "sanity-codegen": "0.9.8",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.0"
  }
}
