{
  "name": "@navikt/ds-react",
  "version": "1.0.0",
  "description": "NAV designsystem react components",
  "author": "NAV Designsystem team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/Designsystemet.git"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "_docs.json"
  ],
  "scripts": {
    "docgen": "yarn ts-node ../../../scripts/docgen.ts",
    "create-package-json-pointers-to-esm": "node ../../../scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm",
    "build": "yarn run clean && concurrently \"tsc\" \"tsc -p tsconfig.esm.json\" && yarn create-package-json-pointers-to-esm",
    "watch": "tsc --watch -p tsconfig.esm.json",
    "test": "jest"
  },
  "dependencies": {
    "@floating-ui/react-dom": "0.6.0",
    "@material-ui/core": "^4.12.3",
    "@navikt/ds-icons": "^1.0.0",
    "@popperjs/core": "^2.10.1",
    "@radix-ui/react-tabs": "0.1.5",
    "@radix-ui/react-toggle-group": "0.1.5",
    "classnames": "^2.2.6",
    "react-collapse": "^5.1.0",
    "react-merge-refs": "^1.1.0",
    "react-modal": "3.14.3",
    "react-popper": "^2.2.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/faker": "5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.35",
    "@types/react-collapse": "^5.0.1",
    "@types/react-modal": "^3.13.1",
    "concurrently": "7.2.1",
    "copyfiles": "^2.4.1",
    "faker": "5.5.3",
    "fast-glob": "3.2.11",
    "jest": "^27.5.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "3.0.2",
    "swr": "^1.1.2",
    "ts-jest": "^27.1.0",
    "ts-node": "10.7.0",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  }
}
